 <h1>Props</h1>
<div class="hero-unit">


  <table class="table table-striped table-bordered table-condensed">
  <thead>
  <tr>
  <th>Name</th>
   <th>Options</th>
  </tr>
  </thead>
  <tbody>
  <%
  for (var p in props) { %>

    <tr>
    <td><strong><%=p%></strong></td>
    <td>
    <a title="view" class="icon-eye-open" href="/locations/<%=props[p].name %>"></a>
    <a title="edit" class="icon-pencil" href="/locations/<%=props[p].name %>/edit"></a>
    <a title="remove" class="icon-remove" href="/locations/<%=props[p].name %>"></a>
    </td>
    </tr>
  <% } %>

  </tbody>
  </table>

<a class="btn btn-primary" href="/locations/add">Add Location</a>
</div>
